{
  "manifest_version": 2,
  "name": "CNB_scrapper",
  "version": "0.2",
  "description": "Scrap CNB content and it delivers it in a more friendly format. You can select between posted transactions or in progress ones",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "clipboardWrite",
    "https://cno.cnb.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon-96.png",
    "default_title": "CNB Scrap",
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{dac587ec-51d6-4f5d-98bb-70182b0738aa}"
    }
  }
}
